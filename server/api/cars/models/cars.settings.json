{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "name": "Carros",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "brand": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true
    },
    "seats": {
      "type": "integer",
      "required": true
    },
    "mode": {
      "type": "enumeration",
      "enum": [
        "manual",
        "auto"
      ],
      "required": true
    },
    "gas": {
      "type": "enumeration",
      "enum": [
        "diesel",
        "gasolina",
        "eletrico",
        "hibrido"
      ]
    },
    "doors": {
      "type": "integer"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "mota",
        "ligeiro",
        "comercial",
        "minibus"
      ],
      "required": true
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "fleet": {
      "type": "enumeration",
      "enum": [
        "continente",
        "madeira",
        "acores"
      ],
      "default": "continente"
    },
    "cc": {
      "type": "string"
    },
    "seasonalPrice": {
      "type": "component",
      "repeatable": true,
      "component": "season.season"
    }
  }
}
