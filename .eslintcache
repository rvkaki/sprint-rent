;\r\n  const [locations, setLocations] = useState([]);\r\n  const [grid, setGrid] = useState(false);\r\n  const appState = useContext(AppContext);\r\n\r\n  const { isOpen, onOpen, onClose } = useDisclosure();\r\n\r\n  const history = useHistory();\r\n\r\n  useEffect(() => {\r\n    getLocations().then(data => setLocations(data));\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    const setStateFromQuery = async () => {\r\n      const [pickup, delivery, date] = history.location.search\r\n        .slice(1)\r\n        .split('&');\r\n      appState.setStartLocation(parseInt(pickup.split('=')[1]));\r\n      appState.setEndLocation(parseInt(delivery.split('=')[1]));\r\n      const [startDate, endDate] = date.split('=')[1].split('-');\r\n      // convert from DD/MM/YYYY to MM/DD/YYYY to create date\r\n      appState.setStartDate(\r\n        new Date(\r\n          `${startDate.split('/')[1]}/${startDate.split('/')[0]}/${\r\n            startDate.split('/')[2]\r\n          }`\r\n        )\r\n      );\r\n      appState.setEndDate(\r\n        new Date(\r\n          `${endDate.split('/')[1]}/${endDate.split('/')[0]}/${\r\n            endDate.split('/')[2]\r\n          }`\r\n        )\r\n      );\r\n    };\r\n\r\n    const clearState = async () => {\r\n      appState.setEndDate(null);\r\n      appState.setEndLocation(null);\r\n      appState.setStartDate(null);\r\n      appState.setStartLocation(null);\r\n    };\r\n\r\n    if (history.location.search !== '')\r\n      setStateFromQuery();\r\n    else clearState();\r\n  }, [history.location.search]);\r\n\r\n  useEffect(() => {\r\n    let query = '?';\r\n    for (const key in appState.filters) {\r\n      const elem = appState.filters[key];\r\n      let elemQuery = '';\r\n      if (elem.length > 0) {\r\n        elem.forEach(x => (elemQuery += `${key}_in=${x}&`));\r\n        query += elemQuery;\r\n      }\r\n    }\r\n    query = query.slice(0, -1);\r\n    getCars(query).then(data => setCars(data));\r\n  }, [appState.filters]);\r\n\r\n  const selectCar = id => {\r\n    appState.setCar(id);\r\n    if (appState.startLocation) {\r\n      history.push('/checkout');\r\n    } else onOpen();\r\n  };\r\n\r\n  return (\r\n    <Flex direction=\"column\">\r\n      <Header />\r\n      {/* Progresso da reserva */}\r\n      {appState.startLocation && locations.length > 0 ? (\r\n        <Progress locations={locations} state={appState} />\r\n      ) : null}\r\n      <ViewToggle grid={grid} setGrid={setGrid} />\r\n      <Flex\r\n        w=\"100%\"\r\n        direction={{ base: 'column', lg: 'row' }}\r\n        px={{ base: 0, lg: '5%', xl: '8%' }}\r\n        pb={16}\r\n        justify=\"space-between\"\r\n      >\r\n        <Filters />\r\n\r\n        {cars ? (\r\n          cars.length === 0 ? (\r\n            <Box m=\"auto\">\r\n              <Text fontSize=\"2xl\" color=\"black\">\r\n                Sem Resultados\r\n              </Text>\r\n            </Box>\r\n          ) : (\r\n            <>\r\n              {/* Renders on <= medium-sized screens */}\r\n              <Box\r\n                justifyContent=\"center\"\r\n                display={{ base: 'inherit', lg: 'none' }}\r\n              >\r\n                <Grid\r\n                  templateColumns={{ base: '1fr', md: '1fr 1fr' }}\r\n                  gap={6}\r\n                  w=\"90%\"\r\n                >\r\n                  {cars.map(car => (\r\n                    <CarCard grid key={car.id} car={car} select={selectCar} />\r\n                  ))}\r\n                </Grid>\r\n              </Box>\r\n\r\n              {/* Renders on > medium-sized screens */}\r\n              <Box\r\n                display={{ base: 'none', lg: 'inherit' }}\r\n                flex={1}\r\n                justifyContent=\"flex-end\"\r\n              >\r\n                {grid ? (\r\n                  <Grid templateColumns=\"repeat(2, 1fr)\" gap={6} w=\"80%\">\r\n                    {cars.map(car => (\r\n                      <CarCard grid key={car.id} car={car} select={selectCar} />\r\n                    ))}\r\n                  </Grid>\r\n                ) : (\r\n                  <Stack align=\"center\" spacing={5} w=\"80%\">\r\n                    {cars.map(car => (\r\n                      <CarCard key={car.id} car={car} select={selectCar} />\r\n                    ))}\r\n                  </Stack>\r\n                )}\r\n              </Box>\r\n            </>\r\n          )\r\n        ) : (\r\n          <Box m=\"auto\">\r\n            <Spinner />\r\n          </Box>\r\n        )}\r\n      </Flex>\r\n\r\n      <DataPopUp\r\n        options={locations.map(l => {\r\n          return { name: l.title, value: l.id };\r\n        })}\r\n        isOpen={isOpen}\r\n        onClose={onClose}\r\n      />\r\n\r\n      <Footer />\r\n    </Flex>\r\n  );\r\n};\r\n\r\nexport default Frota;\r\n","C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\views\\Home.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\CarCard.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\Footer.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\Filters.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\Header.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\MainSearch.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\Locations.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\SearchBar.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\util\\preloader.js",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\CustomDrawer.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\ViewToggle.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\components\\InfoCard.jsx",[],"C:\\Users\\Ricardo\\Documents\\Projects\\sprint-rent\\src\\util\\apiCalls.js",[],"C:\\Users\\Ricardo\\Doc